<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework> <!-- Specify your target framework -->
    <PackageId>example-nuget-package-4</PackageId> <!-- Replace with your package ID -->
    <Version>1.0.1</Version> <!-- Replace with your version -->
    <Authors>git-package-master</Authors> <!-- Replace with your name or organization -->
    <Company>Your Company</Company> <!-- Replace with your company name -->
    <Description>A simple nuget package.</Description> <!-- Replace with your description -->
    <RepositoryUrl>https://github.com/GIT-Package-Master/Github-Nuget-Package-Examples</RepositoryUrl> <!-- Replace with your repository URL -->
    <PackageLicenseExpression>MIT</PackageLicenseExpression> <!-- Specify your license -->
    <PackageProjectUrl>https://github.com/GIT-Package-Master/Github-Nuget-Package-Examples</PackageProjectUrl> <!-- Replace with your project URL -->
    <PackageTags>v1.0.0</PackageTags> <!-- Replace with relevant tags -->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json " Version="1.0.0" /> <!-- Add any dependencies here -->
  </ItemGroup>

</Project>
